bindkey \
  '^[[3~' delete-char \
  '^[[H' beginning-of-line \
  '^[[F' end-of-line \
  '^[[5~' history-search-backward \
  '^[[6~' history-search-forward \
  '^H' backward-kill-word \
  '5~' kill-word \
  '^[[1;5C' forward-word \
  '^[[1;5D' backward-word

bindkey -s \
  '^[[1~' '^[[H' \
  '^[[4~' '^[[F'
